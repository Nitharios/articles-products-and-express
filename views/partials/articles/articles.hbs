<h1>Articles</h1>

<h2>Add New Article</h2>

<form action="/articles/new" method="get">
  <input type="submit" value="ADD">
</form>

<h2>List of Articles</h2>

<ul>
  {{#each data}}
    <li>
      Title: {{title}}
      <br>Author: {{author}}
      <p>{{body}}
      <p><form action="/articles/{{uri}}/edit" method="get">
        <input type="submit" value="EDIT"></p>
      </form>
    </li>
  {{/each}}
</ul>